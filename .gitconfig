
[user]

[core]
  pager = less -r
  editor = vim

[alias]
  a  = add
  b  = branch
  c  = commit
  dg = pull --rebase
  f  = fetch
  k  = checkout
  l  = log
  ll = log --pretty=format:"%C(yellow)%h%Cred%d\\ %Creset%s%Cblue\\ [%cn]" --decorate --numstat
  m  = merge
  p  = push
  pl = pull
  rv = revert
  rs = reset
  s  = status
  
[color] 
  ui = auto
 
